mpirun --hostfile hostfile -np 2 ./$dist_dir/bin/caffe.bin train --solver=examples/cifar10/cifar10_quick_solver_m0_mpi.prototxt 2>>./results/cifar10_m0/cifar10_quick_m0_2.log

